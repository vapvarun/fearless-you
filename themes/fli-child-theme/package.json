    "lint:js:fix": "eslint '**/*.js' --ignore-pattern 'node_modules/' --ignore-pattern '*.min.js' --fix",
    "lint:php": "phpcs --standard=phpcs.xml .",
    "lint:php:fix": "phpcbf --standard=phpcs.xml ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vapvarun/fli-child-theme.git"
  },
