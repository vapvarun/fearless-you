  "bugs": {
    "url": "https://github.com/vapvarun/fli-child-theme/issues"
  },
  "homepage": "https://github.com/vapvarun/fli-child-theme#readme",
  "devDependencies": {
    "@wordpress/eslint-plugin": "^22.19.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jsdoc": "^61.1.4"
  }
