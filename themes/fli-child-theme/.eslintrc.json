{
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended"
  ],
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "globals": {
    "wp": "readonly",
    "ajaxurl": "readonly",
    "_wpUtilSettings": "readonly",
    "jQuery": "readonly",
    "$": "readonly",
    "learndash_video_data": "readonly",
    "ld_video_players": "readonly",
    "LearnDash_disable_assets": "readonly",
    "ldVars": "readonly"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "no-undef": "error",
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", "tab"]
  }
}
